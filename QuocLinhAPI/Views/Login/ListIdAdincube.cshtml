@using BUS
@model dynamic
@{
}

@section css{
    

}

@section js{
    
}

<div class="right_col" role="main" style="min-height: 3539px;">
    <div class="">
        <div class="clearfix"></div>
        <div class="row">
            <div class="col-md-12 col-sm-12 col-xs-12">
                <div class="x_panel">
                    <div class="x_title">
                        <h2>Lấy danh sách ID Adincube</h2>
                        <div class="clearfix"></div>
                    </div>
                    <div class="x_content form-horizontal">
                        <br>
                        <form method="post" class="form">
                            <div class="form-group">
                                <label class="control-label col-md-2 col-sm-2 col-xs-12" for="last-name">
                                    Token<span class="required">*</span>
                                </label>
                                <div class="col-md-4 col-sm-4 col-xs-12">
                                    <input class="form-control col-md-7 col-xs-12 text-box single-line" required="required" type="text" name="Token">
                                </div>
                            </div>
                            @if (ViewBag.Error != null)
                            {
                                <div class="text text-danger">
                                    @ViewBag.Error
                                </div>
                            }
                            <div class="ln_solid"></div>
                            <div class="form-group">
                                <div class="col-md-6 col-sm-6 col-xs-12 col-md-offset-3">
                                    <button type="submit" class="btn btn-success">Tìm</button>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="">
        <div class="clearfix"></div>
        <div class="row">
            <div class="col-md-12 col-sm-12 col-xs-12">
                <div class="x_panel">
                    <div class="x_title">
                        <h2>Danh sách ID</h2>
                        <div class="clearfix"></div>
                    </div>
                    <div class="x_content">
                        <table id="listing" class="table table-striped table-bordered bulk_action">
                            <thead>
                                <tr>
                                    <th>ID</th>
                                    <th>Platform</th>
                                    <th>Name</th>
                                    <th>Key</th>
                                </tr>
                            </thead>
                            <tbody>
                                @if (Model != null)
                                {
                                    foreach (var item in Model)
                                    {
                                        <tr>
                                            <td>@item["id"]</td>
                                            <td>@item["platform"]</td>
                                            <td>@item["name"]</td>
                                            <td>@item["key"]</td>
                                        </tr>
                                    }

                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
